<p id="notice"><%= notice %></p>

<h1>Pinmages</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pinmages.each do |pinmage| %>
      <tr>
        <td><%= pinmage.title %></td>
        <td><%= pinmage.description %></td>
        <td><%= link_to 'Show', pinmage %></td>
        <td><%= link_to 'Edit', edit_pinmage_path(pinmage) %></td>
        <td><%= link_to 'Destroy', pinmage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
